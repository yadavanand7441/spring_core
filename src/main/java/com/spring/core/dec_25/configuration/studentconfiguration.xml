<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.spring.core.dec_25.entity.Student" id="student1">
        <property name="studentId" value="12341"/>
        <property name="studentName" value="Sonam Kapoor"/>
        <property name="studentAddress" value="Mumbai"/>
    </bean>

    <bean class="com.spring.core.dec_25.entity.Student" id="student2">
        <property name="studentId" value="452432"/>
        <property name="studentName" value="Mohan Rohan"/>
        <property name="studentAddress" value="Patna"/>
    </bean>
</beans>