<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean class="com.core.spring.entity.Student" id="student1">
        <property name="id" value="12312"/>
        <property name="name" value="Tony Kumar"/>
        <property name="address" value="Patna"/>
    </bean>

    <bean class="com.core.spring.entity.Student" id="student2">
        <property name="id" value="251342"/>
        <property name="name" value="Monu Kumar"/>
        <property name="address" value="Gaya"/>
    </bean>


</beans>